<script>
  (function() {
    class NailSizeGuide{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.shapeButtons = this.querySelectorAll('.ai-size-guide-shape-btn-{{ ai_gen_id }}');
        this.contentSections = this.querySelectorAll('.ai-size-guide-content-{{ ai_gen_id }}');
        this.setupEventListeners();
        this.showFirstAvailableShape();
      }

      setupEventListeners() {
        this.shapeButtons.forEach((button) => {
          button.addEventListener('click', (e) => {
            const shape = button.getAttribute('data-shape');
            this.showContent(shape);
            this.setActiveButton(button);
          });
        });
      }

      showFirstAvailableShape() {
        if (this.shapeButtons.length > 0) {
          const firstButton = this.shapeButtons[0];
          const firstShape = firstButton.getAttribute('data-shape');
          this.showContent(firstShape);
          this.setActiveButton(firstButton);
        }
      }

      showContent(shape) {
        this.contentSections.forEach((section) => {
          section.classList.remove('active');
        });

        const targetContent = this.querySelector(`[data-content="${shape}"]`);
        if (targetContent) {
          setTimeout(() => {
            targetContent.classList.add('active');
            targetContent.scrollIntoView({ behavior: 'smooth', block: 'nearest' });
          }, 100);
        }
      }

      setActiveButton(activeButton) {
        this.shapeButtons.forEach((button) => {
          button.classList.remove('active');
        });
        activeButton.classList.add('active');
      }
    }

    customElements.define('nail-size-guide-{{ ai_gen_id }}', NailSizeGuide{{ ai_gen_id }});
  })();
</script>

