{% doc %}
  @prompt
    Create a new, visually appealing click-through “Size Guide” section for my SIYOU Nails theme.
    The goal is to make the page more interactive and user-friendly while keeping the brand’s clean, feminine, and professional aesthetic (light pink accents, minimalist icons, and rounded buttons).
    
    Include these design and UX features:
    
    A step-by-step click-through interface that lets users select their nail shape (e.g., Short Almond, Medium Almond, Long Almond, Medium Coffin, Long Coffin, Medium Stiletto, Long Stiletto).
    
    When a shape is clicked, show a visual size chart and a brief description of fit and curvature tips.
    
    Add a “Find My Size” call-to-action button that links to a contact form or size quiz.
    
    Use scroll or slide animations for transitions between shapes.
    
    Keep typography modern and clean (e.g., Montserrat or Lato), with soft pink highlights (#f8dce2) and neutral grays
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-size-guide-{{ ai_gen_id }} {
    padding: 60px 20px;
    background-color: {{ block.settings.background_color }};
    max-width: 1200px;
    margin: 0 auto;
  }

  .ai-size-guide-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 50px;
  }

  .ai-size-guide-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 15px;
    font-weight: 600;
  }

  .ai-size-guide-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
    line-height: 1.6;
  }

  .ai-size-guide-shapes-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(140px, 1fr));
    gap: 15px;
    margin-bottom: 40px;
    max-width: 900px;
    margin-left: auto;
    margin-right: auto;
  }

  .ai-size-guide-shape-btn-{{ ai_gen_id }} {
    background: {{ block.settings.shape_button_bg }};
    border: 2px solid {{ block.settings.shape_button_border }};
    border-radius: {{ block.settings.button_radius }}px;
    padding: 20px 15px;
    cursor: pointer;
    transition: all 0.3s ease;
    text-align: center;
    color: {{ block.settings.shape_button_text }};
    font-size: 14px;
    font-weight: 500;
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 8px;
  }

  .ai-size-guide-shape-btn-{{ ai_gen_id }}:hover {
    background: {{ block.settings.shape_button_hover_bg }};
    border-color: {{ block.settings.accent_color }};
    transform: translateY(-2px);
    box-shadow: 0 4px 12px rgba(252, 128, 159, 0.15);
  }

  .ai-size-guide-shape-btn-{{ ai_gen_id }}.active {
    background: {{ block.settings.accent_color }};
    border-color: {{ block.settings.accent_color }};
    color: #ffffff;
  }

  .ai-size-guide-shape-icon-{{ ai_gen_id }} {
    width: 40px;
    height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-size-guide-shape-icon-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    fill: currentColor;
  }

  .ai-size-guide-content-{{ ai_gen_id }} {
    display: none;
    opacity: 0;
    transform: translateY(20px);
    transition: opacity 0.4s ease, transform 0.4s ease;
  }

  .ai-size-guide-content-{{ ai_gen_id }}.active {
    display: block;
    opacity: 1;
    transform: translateY(0);
  }

  .ai-size-guide-chart-wrapper-{{ ai_gen_id }} {
    background: #ffffff;
    border-radius: {{ block.settings.content_radius }}px;
    padding: 40px;
    box-shadow: 0 2px 20px rgba(0, 0, 0, 0.06);
    margin-bottom: 30px;
  }

  .ai-size-guide-chart-title-{{ ai_gen_id }} {
    font-size: 22px;
    color: {{ block.settings.title_color }};
    margin: 0 0 25px;
    text-align: center;
    font-weight: 600;
  }

  .ai-size-guide-table-{{ ai_gen_id }} {
    width: 100%;
    border-collapse: collapse;
    margin-bottom: 25px;
  }

  .ai-size-guide-table-{{ ai_gen_id }} th {
    background: {{ block.settings.table_header_bg }};
    color: {{ block.settings.table_header_text }};
    padding: 15px;
    text-align: left;
    font-weight: 600;
    font-size: 14px;
    text-transform: uppercase;
    letter-spacing: 0.5px;
  }

  .ai-size-guide-table-{{ ai_gen_id }} td {
    padding: 15px;
    border-bottom: 1px solid #f0f0f0;
    color: {{ block.settings.subtitle_color }};
    font-size: 15px;
  }

  .ai-size-guide-table-{{ ai_gen_id }} tr:last-child td {
    border-bottom: none;
  }

  .ai-size-guide-description-{{ ai_gen_id }} {
    background: {{ block.settings.description_bg }};
    border-left: 4px solid {{ block.settings.accent_color }};
    padding: 20px 25px;
    border-radius: 8px;
    margin-bottom: 20px;
  }

  .ai-size-guide-description-{{ ai_gen_id }} h4 {
    color: {{ block.settings.title_color }};
    margin: 0 0 10px;
    font-size: 16px;
    font-weight: 600;
  }

  .ai-size-guide-description-{{ ai_gen_id }} p {
    color: {{ block.settings.subtitle_color }};
    margin: 0;
    line-height: 1.6;
    font-size: 15px;
  }

  .ai-size-guide-cta-{{ ai_gen_id }} {
    text-align: center;
    margin-top: 40px;
  }

  .ai-size-guide-cta-btn-{{ ai_gen_id }} {
    display: inline-block;
    background: {{ block.settings.cta_bg }};
    color: {{ block.settings.cta_text_color }};
    padding: 16px 40px;
    border-radius: {{ block.settings.button_radius }}px;
    text-decoration: none;
    font-weight: 600;
    font-size: 15px;
    transition: all 0.3s ease;
    border: none;
    cursor: pointer;
    text-transform: uppercase;
    letter-spacing: 1px;
  }

  .ai-size-guide-cta-btn-{{ ai_gen_id }}:hover {
    background: {{ block.settings.cta_hover_bg }};
    transform: translateY(-2px);
    box-shadow: 0 6px 20px rgba(252, 128, 159, 0.3);
  }

  .ai-size-guide-empty-state-{{ ai_gen_id }} {
    text-align: center;
    padding: 60px 20px;
    color: {{ block.settings.subtitle_color }};
  }

  .ai-size-guide-empty-state-{{ ai_gen_id }} svg {
    width: 80px;
    height: 80px;
    margin-bottom: 20px;
    opacity: 0.3;
  }

  @media screen and (max-width: 768px) {
    .ai-size-guide-{{ ai_gen_id }} {
      padding: 40px 15px;
    }

    .ai-size-guide-shapes-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);
      gap: 10px;
    }

    .ai-size-guide-chart-wrapper-{{ ai_gen_id }} {
      padding: 25px 20px;
    }

    .ai-size-guide-table-{{ ai_gen_id }} {
      font-size: 13px;
    }

    .ai-size-guide-table-{{ ai_gen_id }} th,
    .ai-size-guide-table-{{ ai_gen_id }} td {
      padding: 12px 10px;
    }

    .ai-size-guide-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.8 }}px;
    }
  }
{% endstyle %}

